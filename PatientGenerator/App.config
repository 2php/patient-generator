<configuration>
  <configSections>
    <section name="marc.hi.ehrs.svc.core" type="MARC.HI.EHRS.SVC.Core.HostConfigurationSectionHandler, MARC.HI.EHRS.SVC.Core, Version=1.0.0.0"/>
    <section name="medic.patientgen.hl7v2" type="PatientGenerator.HL7v2.Configuration.ConfigurationSectionHandler, PatientGenerator.HL7v2, Version=1.0.0.0" />
  </configSections>
  <marc.hi.ehrs.svc.core>
    <serviceAssemblies>
      <add assembly="MARC.HI.EHRS.SVC.Core.dll" />
      <add assembly="PatientGenerator.Core.dll" />
      <add assembly="PatientGenerator.FHIR.dll" />
      <add assembly="PatientGenerator.HL7v2.dll" />
      <add assembly="PatientGenerator.HL7v3.dll" />
      <add assembly="PatientGenerator.Messaging.dll" />
    </serviceAssemblies>
    <serviceProviders>
      <add type="PatientGenerator.Messaging.MessageHandlerService, PatientGenerator.Messaging, Version=1.0.0.0"/>
    </serviceProviders>
  </marc.hi.ehrs.svc.core>
  <medic.patientgen.hl7v2>
    <endpoints>
      <endpoint name="cr" address="mllp://il.aehin.marc-hi.ca:2100" />
    </endpoints>
  </medic.patientgen.hl7v2>
  <system.diagnostics>
    <sharedListeners>
      <add name="rollOver" type="MARC.HI.EHRS.SVC.Core.Logging.RollOverTextWriterTraceListener, MARC.HI.EHRS.SVC.Core, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" initializeData="hns.log">
        <filter type="System.Diagnostics.EventTypeFilter" initializeData="Error, Information, Warning" />
      </add>
    </sharedListeners>
    <trace autoflush="true">
      <listeners>
        <!--<add name="rollOver" />-->
        <add name="consoleListener" type="System.Diagnostics.ConsoleTraceListener" />
      </listeners>
    </trace>
  </system.diagnostics>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://cr.aehin.marc-hi.ca:8080/cr" binding="basicHttpBinding"
        contract="MARC.Everest.Connectors.WCF.Core.IConnectorContract"
        name="cr" />
    </client>
  </system.serviceModel>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6" />
  </startup>
</configuration>